{
    "name": "profify",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "mock": "NEXT_PUBLIC_MOCKDATA=TRUE npm run dev",
        "dev": "NEXT_PUBLIC_MODE=DEV next dev",
        "start": "next start",
        "build": "next build",
        "storybook": "storybook dev -p 6006",
        "storybook:nocache": "NEXT_PUBLIC_MODE=DEV NEXT_PUBLIC_MOCKDATA=TRUE start-storybook -p 6006 --no-manager-cache",
        "build-storybook": "storybook build",
        "export": "rm -rf ./out && next build && next export",
        "export-dev": "rm -rf ./out && NEXT_PUBLIC_MODE=DEV next build && NEXT_PUBLIC_MODE=DEV next export",
        "export-mock": "rm -rf ./out && NEXT_PUBLIC_MODE=DEV NEXT_PUBLIC_MOCKDATA=TRUE next build && NEXT_PUBLIC_MODE=DEV NEXT_PUBLIC_MOCKDATA=TRUE next export",
        "standard": "npx standard",
        "sass": "sass ./styles/style.scss:./styles/style.css",
        "init-cap": "npx cap init",
        "sync": "npx cap sync",
        "resources": "capacitor-resources -p android,ios",
        "generate-android-icon": "cordova-res android --skip-config --copy",
        "build-android": "rm -rf ./android && npx cap add android",
        "build-android-dev": "rm -rf ./android && NEXT_PUBLIC_MODE=DEV npx cap add android",
        "build-ios": "rm -rf ./ios && npx cap add ios",
        "build-ios-dev": "rm -rf ./ios && NEXT_PUBLIC_MODE=DEV npx cap add ios",
        "lint-next": "next lint --no-cache",
        "lint": "npx eslint src",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier src styles '!**/*.stories.tsx' --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "fle": "npm run format && npm run lint && npm run export",
        "flb": "npm run format && npm run lint && npm run build"
    },
    "dependencies": {
        "@capacitor/android": "^5.0.4",
        "@capacitor/cli": "^5.0.4",
        "@capacitor/core": "^5.0.4",
        "@capacitor/ios": "^5.0.4",
        "axios": "^1.4.0",
        "fenextjs": "^1.0.46",
        "next": "12.2.5",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "yup": "^1.0.0"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^7.0.17",
        "@storybook/addon-essentials": "^7.0.17",
        "@storybook/addon-interactions": "^7.0.17",
        "@storybook/addon-links": "^7.0.17",
        "@storybook/blocks": "^7.0.18",
        "@storybook/builder-webpack5": "^7.0.17",
        "@storybook/manager-webpack5": "^6.5.16",
        "@storybook/nextjs": "^7.0.17",
        "@storybook/react": "^7.0.17",
        "@storybook/testing-library": "^0.1.0",
        "@types/node": "18.7.5",
        "@types/react": "18.0.17",
        "@types/react-dom": "18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "8.24.0",
        "eslint-config-next": "^13.4.4",
        "eslint-plugin-storybook": "^0.6.12",
        "prettier": "^2.7.1",
        "storybook": "^7.0.17",
        "storybook-dark-mode": "^2.1.1",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.4"
    }
}
